<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Enseignant" table="enseignant">
    <id name="idpers" association-key="true"/>
    <field name="nom" type="string" column="NOM" length="32" nullable="true"/>
    <field name="prenom" type="string" column="PRENOM" length="32" nullable="true"/>
    <field name="situation" type="string" column="SITUATION" length="32" nullable="true"/>
    <field name="adresse" type="string" column="ADRESSE" length="32" nullable="true"/>
    <one-to-one field="idpers" target-entity="Intervenant">
      <join-columns>
        <join-column name="IDPERS" referenced-column-name="IDPERS"/>
      </join-columns>
    </one-to-one>
  </entity>
</doctrine-mapping>
