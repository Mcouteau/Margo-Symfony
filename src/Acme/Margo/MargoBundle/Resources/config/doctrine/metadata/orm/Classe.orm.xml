<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Classe" table="classe">
    <indexes>
      <index name="I_FK_CLASSE_FILIERE" columns="CODEFILIERE"/>
    </indexes>
    <id name="codeclasse" type="string" column="CODECLASSE" length="32">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="libclasse" type="string" column="LIBCLASSE" length="32" nullable="true"/>
    <many-to-one field="codefiliere" target-entity="Filiere">
      <join-columns>
        <join-column name="CODEFILIERE" referenced-column-name="CODEFILIERE"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
