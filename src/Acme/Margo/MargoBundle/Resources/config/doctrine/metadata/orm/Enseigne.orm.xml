<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Enseigne" table="enseigne">
    <indexes>
      <index name="I_FK_ENSEIGNE_INTERVENANT" columns="IDPERS"/>
      <index name="I_FK_ENSEIGNE_MATIERE" columns="CODEMAT"/>
      <index name="I_FK_ENSEIGNE_CLASSE" columns="CODECLASSE"/>
    </indexes>
    <id name="idpers" association-key="true"/>
    <id name="codemat" association-key="true"/>
    <id name="codeclasse" association-key="true"/>
    <one-to-one field="codeclasse" target-entity="Classe">
      <join-columns>
        <join-column name="CODECLASSE" referenced-column-name="CODECLASSE"/>
      </join-columns>
    </one-to-one>
    <one-to-one field="codemat" target-entity="Matiere">
      <join-columns>
        <join-column name="CODEMAT" referenced-column-name="CODEMAT"/>
      </join-columns>
    </one-to-one>
    <one-to-one field="idpers" target-entity="Intervenant">
      <join-columns>
        <join-column name="IDPERS" referenced-column-name="IDPERS"/>
      </join-columns>
    </one-to-one>
  </entity>
</doctrine-mapping>
