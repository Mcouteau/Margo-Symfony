<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Enfant" table="enfant">
    <indexes>
      <index name="I_FK_ENFANT_PERSONNEL" columns="IDPERS"/>
    </indexes>
    <id name="idenfant" type="string" column="IDENFANT" length="32">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="prenom" type="string" column="PRENOM" length="32" nullable="true"/>
    <field name="datenaiss" type="string" column="DATENAISS" length="32" nullable="true"/>
    <many-to-one field="idpers" target-entity="Personnel">
      <join-columns>
        <join-column name="IDPERS" referenced-column-name="IDPERS"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
